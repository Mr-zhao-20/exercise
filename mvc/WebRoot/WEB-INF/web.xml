<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Login</servlet-name>
    <servlet-class>com.my.servlet.Login</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginAdd</servlet-name>
    <servlet-class>com.my.servlet.LoginAdd</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Loginlist</servlet-name>
    <servlet-class>com.my.servlet.Loginlist</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginGet</servlet-name>
    <servlet-class>com.my.servlet.LoginGet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginUpdate</servlet-name>
    <servlet-class>com.my.servlet.LoginUpdate</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login.bin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginAdd</servlet-name>
    <url-pattern>/LoginAdd.bin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Loginlist</servlet-name>
    <url-pattern>/Loginlist.bin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginGet</servlet-name>
    <url-pattern>/LoginGet.bin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginUpdate</servlet-name>
    <url-pattern>/LoginUpdate.bin</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
